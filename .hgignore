syntax: regexp
# The recurrent (^|/) idiom in the regexps below should be understood
# to mean "at any directory" while the ^ idiom means "from the
# project's top-level directory".

(^|/)octave-(core|workspace)$
\.oct$
\.a$
\.o$
\.Po$
fntests.log

# The following files are created during package build
^build/

# aclocal
/aclocal.m4
/autom4te.cache/

# autoheader
^src/crlibm/(.*)\.in$

# autoconf
^src/(.*/)?configure$

# automake
^src/crlibm/(INSTALL|compile|config.guess|config.sub|depcomp|install-sh|missing|test-driver)$

# configure
^src/(.*/)?(Makefile|config.log|config.status|crlibm_config.h|scs_lib/Makefile|stamp-h1|tests/Makefile)$

# test suite
^src/crlibm/tests/crlibm_
