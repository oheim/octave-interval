%!shared
%!  ## Disable warnings when creating NaIs
%!  warning ("off", "interval:NaI")
%! ## Test library imports
%!function s = to_string (x)
%!    if (isnumeric (x))
%!        s = num2str (x);
%!    elseif (isa (x, 'infsup'))
%!        s = intervaltotext (x);
%!    else
%!        # x probably is a char.
%!        s = x;
%!    endif
%!endfunction
%
%!function assert_equals (x, y)
%!    if (isa (x, "infsup"))
%!        assert (eq (x, y));
%!    else
%!        assert (isequal (x, y));
%!    endif
%!endfunction
%
%!function n = decval (dec)
%! n = find (strcmp (dec, {"ill", "trv", "def", "dac", "com"}));
%!endfunction

%!test "minimal_empty_test";
%! assert_equals (...
%!    isempty (infsup ()), ...
%!    true)
%! assert_equals (...
%!    isempty (infsup (-inf, inf)), ...
%!    false)
%! assert_equals (...
%!    isempty (infsup (1.0, 2.0)), ...
%!    false)
%! assert_equals (...
%!    isempty (infsup (-1.0, 2.0)), ...
%!    false)
%! assert_equals (...
%!    isempty (infsup (-3.0, -2.0)), ...
%!    false)
%! assert_equals (...
%!    isempty (infsup (-inf, 2.0)), ...
%!    false)
%! assert_equals (...
%!    isempty (infsup (-inf, 0.0)), ...
%!    false)
%! assert_equals (...
%!    isempty (infsup (-inf, -0.0)), ...
%!    false)
%! assert_equals (...
%!    isempty (infsup (0.0, inf)), ...
%!    false)
%! assert_equals (...
%!    isempty (infsup (-0.0, inf)), ...
%!    false)
%! assert_equals (...
%!    isempty (infsup (-0.0, 0.0)), ...
%!    false)
%! assert_equals (...
%!    isempty (infsup (0.0, -0.0)), ...
%!    false)
%! assert_equals (...
%!    isempty (infsup (0.0, 0.0)), ...
%!    false)
%! assert_equals (...
%!    isempty (infsup (-0.0, -0.0)), ...
%!    false)

