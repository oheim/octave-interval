%!test "minimal_equal_test";
%! assert_equals (...
%!    eq (infsup (1.0, 2.0), infsup (1.0, 2.0)), ...
%!    true)

%! assert_equals (...
%!    infsup (1.0, 2.0) == infsup (1.0, 2.0), ...
%!    true)
%! assert_equals (...
%!    eq (infsup (1.0, 2.1), infsup (1.0, 2.0)), ...
%!    false)

%! assert_equals (...
%!    infsup (1.0, 2.1) == infsup (1.0, 2.0), ...
%!    false)
%! assert_equals (...
%!    eq (infsup (), infsup ()), ...
%!    true)

%! assert_equals (...
%!    infsup () == infsup (), ...
%!    true)
%! assert_equals (...
%!    eq (infsup (), infsup (1.0, 2.0)), ...
%!    false)

%! assert_equals (...
%!    infsup () == infsup (1.0, 2.0), ...
%!    false)
%! assert_equals (...
%!    eq (infsup (-inf, inf), infsup (-inf, inf)), ...
%!    true)

%! assert_equals (...
%!    infsup (-inf, inf) == infsup (-inf, inf), ...
%!    true)
%! assert_equals (...
%!    eq (infsup (1.0, 2.4), infsup (-inf, inf)), ...
%!    false)

%! assert_equals (...
%!    infsup (1.0, 2.4) == infsup (-inf, inf), ...
%!    false)
%! assert_equals (...
%!    eq (infsup (1.0, inf), infsup (1.0, inf)), ...
%!    true)

%! assert_equals (...
%!    infsup (1.0, inf) == infsup (1.0, inf), ...
%!    true)
%! assert_equals (...
%!    eq (infsup (1.0, 2.4), infsup (1.0, inf)), ...
%!    false)

%! assert_equals (...
%!    infsup (1.0, 2.4) == infsup (1.0, inf), ...
%!    false)
%! assert_equals (...
%!    eq (infsup (-inf, 2.0), infsup (-inf, 2.0)), ...
%!    true)

%! assert_equals (...
%!    infsup (-inf, 2.0) == infsup (-inf, 2.0), ...
%!    true)
%! assert_equals (...
%!    eq (infsup (-inf, 2.4), infsup (-inf, 2.0)), ...
%!    false)

%! assert_equals (...
%!    infsup (-inf, 2.4) == infsup (-inf, 2.0), ...
%!    false)
%! assert_equals (...
%!    eq (infsup (-2.0, 0.0), infsup (-2.0, 0.0)), ...
%!    true)

%! assert_equals (...
%!    infsup (-2.0, 0.0) == infsup (-2.0, 0.0), ...
%!    true)
%! assert_equals (...
%!    eq (infsup (-0.0, 2.0), infsup (0.0, 2.0)), ...
%!    true)

%! assert_equals (...
%!    infsup (-0.0, 2.0) == infsup (0.0, 2.0), ...
%!    true)
%! assert_equals (...
%!    eq (infsup (-0.0, -0.0), infsup (0.0, 0.0)), ...
%!    true)

%! assert_equals (...
%!    infsup (-0.0, -0.0) == infsup (0.0, 0.0), ...
%!    true)
%! assert_equals (...
%!    eq (infsup (-0.0, 0.0), infsup (0.0, 0.0)), ...
%!    true)

%! assert_equals (...
%!    infsup (-0.0, 0.0) == infsup (0.0, 0.0), ...
%!    true)
%! assert_equals (...
%!    eq (infsup (0.0, -0.0), infsup (0.0, 0.0)), ...
%!    true)

%! assert_equals (...
%!    infsup (0.0, -0.0) == infsup (0.0, 0.0), ...
%!    true)

