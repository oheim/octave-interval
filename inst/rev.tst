%!    sqrrev (infsupdec (0.0, "0X1.FFFFFFFFFFFE1P+1", "dac"), infsupdec (-0.1, inf, "dac")), ...
%!    infsupdec (-0.1, "0x1.ffffffffffff1p+0", "trv"))

%! assert_equals (...
