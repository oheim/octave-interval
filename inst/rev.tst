%! assert_equals (...
%!    (decorationpart (sqrrev (infsupdec (1.0, 25.0, "dac"), infsupdec (0.0, 7.0, "def"))) {1}), ...
%!    (decorationpart (infsupdec (1.0, 5.0, "trv")) {1}))
