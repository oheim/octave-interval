%!    sqrrev (infsupdec (-1.0, 25.0, "dac")), ...
%!    infsupdec (-5.0, 5.0, "trv"))

%! assert_equals (...
%!    (decorationpart (sqrrev (infsupdec (-1.0, 25.0, "dac"))) {1}), ...
%!    (decorationpart (infsupdec (-5.0, 5.0, "trv")) {1}))
%! assert_equals (...
%!    sqrrev (infsupdec ("0X1.47AE147AE147BP-7", "0X1.47AE147AE147CP-7", "com")), ...
%!    infsupdec ("-0X1.999999999999BP-4", "0X1.999999999999BP-4", "trv"))

%! assert_equals (...
%!    (decorationpart (sqrrev (infsupdec ("0X1.47AE147AE147BP-7", "0X1.47AE147AE147CP-7", "com"))) {1}), ...
%!    (decorationpart (infsupdec ("-0X1.999999999999BP-4", "0X1.999999999999BP-4", "trv")) {1}))
%! assert_equals (...
%!    sqrrev (infsupdec (0.0, "0X1.FFFFFFFFFFFE1P+1", "def")), ...
%!    infsupdec ("-0x1.ffffffffffff1p+0", "0x1.ffffffffffff1p+0", "trv"))

%! assert_equals (...
%!    (decorationpart (sqrrev (infsupdec (0.0, "0X1.FFFFFFFFFFFE1P+1", "def"))) {1}), ...
%!    (decorationpart (infsupdec ("-0x1.ffffffffffff1p+0", "0x1.ffffffffffff1p+0", "trv")) {1}))

