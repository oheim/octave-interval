%!shared
%!  ## Disable warnings when creating NaIs
%!  warning ("off", "interval:NaI")
%! ## Test library imports
%!function s = to_string (x)
%!    if (isnumeric (x))
%!        s = num2str (x);
%!    elseif (isa (x, 'infsup'))
%!        s = intervaltotext (x);
%!    else
%!        # x probably is a char.
%!        s = x;
%!    endif
%!endfunction
%
%!function assert_equals (x, y)
%!    if (isa (x, "infsup"))
%!        assert (eq (x, y));
%!    else
%!        assert (isequal (x, y));
%!    endif
%!endfunction
%
%!function n = decval (dec)
%! n = find (strcmp (dec, {"ill", "trv", "def", "dac", "com"}));
%!endfunction

%!test "minimal_sqrRev_test";
%! assert_equals (...
%!    sqrrev (infsup ()), ...
%!    infsup ())
%! assert_equals (...
%!    sqrrev (infsup (-10.0, -1.0)), ...
%!    infsup ())
%! assert_equals (...
%!    sqrrev (infsup (0.0, inf)), ...
%!    infsup (-inf, inf))
%! assert_equals (...
%!    sqrrev (infsup (0.0, 1.0)), ...
%!    infsup (-1.0, 1.0))
%! assert_equals (...
%!    sqrrev (infsup (-0.5, 1.0)), ...
%!    infsup (-1.0, 1.0))
%! assert_equals (...
%!    sqrrev (infsup (-1000.0, 1.0)), ...
%!    infsup (-1.0, 1.0))
%! assert_equals (...
%!    sqrrev (infsup (0.0, 25.0)), ...
%!    infsup (-5.0, 5.0))
%! assert_equals (...
%!    sqrrev (infsup (-1.0, 25.0)), ...
%!    infsup (-5.0, 5.0))
%! assert_equals (...
%!    sqrrev (infsup ("0X1.47AE147AE147BP-7", "0X1.47AE147AE147CP-7")), ...
%!    infsup ("-0X1.999999999999BP-4", "0X1.999999999999BP-4"))
%! assert_equals (...
%!    sqrrev (infsup (0.0, "0X1.FFFFFFFFFFFE1P+1")), ...
%!    infsup ("-0x1.ffffffffffff1p+0", "0x1.ffffffffffff1p+0"))

%!test "minimal_sqrRevBin_test";
%! assert_equals (...
%!    sqrrev (infsup (), infsup (-5.0, 1.0)), ...
%!    infsup ())
%! assert_equals (...
%!    sqrrev (infsup (-10.0, -1.0), infsup (-5.0, 1.0)), ...
%!    infsup ())
%! assert_equals (...
%!    sqrrev (infsup (0.0, inf), infsup (-5.0, 1.0)), ...
%!    infsup (-5.0, 1.0))
%! assert_equals (...
%!    sqrrev (infsup (0.0, 1.0), infsup (-0.1, 1.0)), ...
%!    infsup (-0.1, 1.0))
%! assert_equals (...
%!    sqrrev (infsup (-0.5, 1.0), infsup (-0.1, 1.0)), ...
%!    infsup (-0.1, 1.0))
%! assert_equals (...
%!    sqrrev (infsup (-1000.0, 1.0), infsup (-0.1, 1.0)), ...
%!    infsup (-0.1, 1.0))
%! assert_equals (...
%!    sqrrev (infsup (0.0, 25.0), infsup (-4.1, 6.0)), ...
%!    infsup (-4.1, 5.0))
%! assert_equals (...
%!    sqrrev (infsup (-1.0, 25.0), infsup (-4.1, 7.0)), ...
%!    infsup (-4.1, 5.0))
%! assert_equals (...
%!    sqrrev (infsup (1.0, 25.0), infsup (0.0, 7.0)), ...
%!    infsup (1.0, 5.0))
%! assert_equals (...
%!    sqrrev (infsup ("0X1.47AE147AE147BP-7", "0X1.47AE147AE147CP-7"), infsup (-0.1, inf)), ...
%!    infsup (-0.1, "0X1.999999999999BP-4"))
%! assert_equals (...
%!    sqrrev (infsup (0.0, "0X1.FFFFFFFFFFFE1P+1"), infsup (-0.1, inf)), ...
%!    infsup (-0.1, "0x1.ffffffffffff1p+0"))

%!test "minimal_sqrRev_dec_test";
