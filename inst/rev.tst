%!    (decorationpart (infsupdec (-1.0, 1.0, "trv")) {1}))
%! assert_equals (...
%!    sqrrev (infsupdec (0.0, 25.0, "def")), ...
%!    infsupdec (-5.0, 5.0, "trv"))

%! assert_equals (...
